set nocompatible              " be iMproved, required
filetype off
filetype plugin indent on
syntax on
set expandtab
set ts=4
set sw=4
set t_Co=256
set cursorline
set nobackup
set number
set ruler
set ignorecase
set autoindent
set whichwrap=b,s,<,>,[,]
set backspace=2
set enc=utf-8
colorscheme zarniwoop
